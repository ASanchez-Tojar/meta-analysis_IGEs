@article{ ISI:000417754100015,
Author = {Signor, Sarah A. and Abbasi, Mohammad and Marjoram, Paul and Nuzhdin,
   Sergey V.},
Title = {{Conservation of social effects () between two species of Drosophila
   despite reversal of sexual dimorphism}},
Journal = {{ECOLOGY AND EVOLUTION}},
Year = {{2017}},
Volume = {{7}},
Number = {{23}},
Pages = {{10031-10041}},
Month = {{DEC}},
Abstract = {{Indirect genetic effects (IGEs) describe the effect of the genes of
   social partners on the phenotype of a focal individual.Here, we measure
   indirect genetic effects using the coefficient of interaction () to test
   whether evolved between Drosophila melanogaster and D.simulans. We
   compare for locomotion between ethanol and nonethanol environments in
   both species, but only D.melanogaster utilizes ethanol ecologically. We
   find that while sexual dimorphism for locomotion has been reversed in
   D.simulans, there has been no evolution of social effects between these
   two species. What did evolve was the interaction between
   genotype-specific and the environment, as D.melanogastervaries
   unpredictably between environments and D.simulansdoes not. In this
   system, this suggests evolutionary lability of sexual dimorphism but a
   conservation of social effects, which brings forth interesting questions
   about the role of the social environment in sexual selection.}},
