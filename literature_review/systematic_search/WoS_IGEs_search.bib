Address = {{1427 E 60TH ST, CHICAGO, IL 60637-2954 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Akcay, E (Reprint Author), Princeton Univ, Dept Ecol \& Evolutionary Biol, Guyot Hall, Princeton, NJ 08544 USA.
   Akcay, Erol, Univ Tennessee, Natl Inst Math \& Biol Synth NIMBioS, Knoxville, TN 37996 USA.
   Van Cleve, Jeremy, Santa Fe Inst, Santa Fe, NM 87501 USA.}},
DOI = {{10.1086/663691}},
ISSN = {{0003-0147}},
Keywords = {{goal-oriented behavior; Price equation; two-tiered model; kin selection;
   multilevel selection; community reciprocity}},
Keywords-Plus = {{SELECTION INCORPORATING INTERACTION; INTERACTING PHENOTYPES; KIN
