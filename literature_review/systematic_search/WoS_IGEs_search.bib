@article{ ISI:000085610800013,
Author = {Chenoweth, PJ},
Title = {{Rationale for using bull breeding soundness evaluations}},
Journal = {{COMPENDIUM ON CONTINUING EDUCATION FOR THE PRACTICING VETERINARIAN}},
Year = {{2000}},
Volume = {{22}},
Number = {{2}},
Pages = {{S48-S55}},
Month = {{FEB}},
Abstract = {{The breeding soundness evaluation (BSE) of bulls is a well-tested,
   cost-effective procedure that categorizes bulls based on their breeding
   potential. Despite a number of proven benefits (positive effects on herd
   fertility, use of fewer bulls, and indirect genetic effects on both male
   and female fertility traits), however, the BSE is underused among cattle
   producers. Bull scrotal circumference, an important aspect of the BSE,
   is moderately to highly heritable and is also favorably associated with
   age at puberty in females and their lifetime productivity. Welfare
   concerns, which focus on semen collection by electroejaculation, are
   discussed in this article, as are supportive arguments to facilitate
   wider acceptance and use of BSEs in bulls.}},
