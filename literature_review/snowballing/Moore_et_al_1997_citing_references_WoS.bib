@article{ ISI:000315034100006,
Author = {Cunningham, Christopher B. and Ruff, James S. and Chase, Kevin and
   Potts, Wayne K. and Carrier, David R.},
Title = {{Competitive Ability in Male House Mice (Mus musculus): Genetic
   Influences}},
Journal = {{BEHAVIOR GENETICS}},
Year = {{2013}},
Volume = {{43}},
Number = {{2}},
Pages = {{151-160}},
Month = {{MAR}},
Abstract = {{Conspecifics of many animal species physically compete to gain
   reproductive resources and thus fitness. Despite the importance of
   competitive ability across the animal kingdom, specific traits that
   influence or underpin competitive ability are poorly characterized.
   Here, we investigate whether there are genetic influences on competitive
   ability within male house mice. Additionally, we examined if litter
   demographics (litter size and litter sex ratio) influence competitive
   ability. We phenotyped two generations for a male's ability to possess a
   reproductive resource--a prime nesting site--using semi-natural
   enclosures with mixed sex groupings. We used the ``Animal Model{''}
   coupled with an extensive pedigree to estimate several genetic
   parameters. Competitive ability was found to be highly heritable, but
   only displayed a moderate genetic correlation to body mass.
   Interestingly, litter sex ratio had a weak negative influence on
   competitive ability. Litter size had no significant influence on
   competitive ability. Our study also highlights how much remains unknown
   about the proximal causes of competitive ability.}},
