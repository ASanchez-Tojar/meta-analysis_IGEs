@article{ ISI:000260410600012,
Author = {Wang, John and Ross, Kenneth G. and Keller, Laurent},
Title = {{Genome-Wide Expression Patterns and the Genetic Architecture of a
   Fundamental Social Trait}},
Journal = {{PLOS GENETICS}},
Year = {{2008}},
Volume = {{4}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{Explaining how interactions between genes and the environment influence
   social behavior is a fundamental research goal, yet there is limited
   relevant information for species exhibiting natural variation in social
   organization. The fire ant Solenopsis invicta is characterized by a
   remarkable form of social polymorphism, with the presence of one or
   several queens per colony and the expression of other phenotypic and
   behavioral differences being completely associated with allelic
   variation at a single Mendelian factor marked by the gene Gp-9.
   Microarray analyses of adult workers revealed that differences in the
   Gp-9 genotype are associated with the differential expression of an
   unexpectedly small number of genes, many of which have predicted
   functions, implying a role in chemical communication relevant to the
   regulation of colony queen number. Even more surprisingly, worker gene
   expression profiles are more strongly influenced by indirect effects
   associated with the Gp-9 genotypic composition within their colony than
   by the direct effect of their own Gp-9 genotype. This constitutes an
   unusual example of an ``extended phenotype'' and suggests a complex
   genetic architecture with a single Mendelian factor, directly and
