@article{ ISI:000075997500009,
Author = {Mousseau, TA and Fox, CW},
Title = {{The adaptive significance of maternal effects}},
Journal = {{TRENDS IN ECOLOGY \& EVOLUTION}},
Year = {{1998}},
Volume = {{13}},
Number = {{10}},
Pages = {{403-407}},
Month = {{OCT}},
Abstract = {{Recently, the adaptive significance of maternal effects has been
   increasingly recognized. No longer are maternal effects relegated as
   simple `troublesome sources of environmental resemblance' that confound
   our ability to estimate! accurately the genetic basis of traits of
   interest. Rather, it has become evident that many maternal effects have
   been shaped by the action of natural selection to act as a mechanism for
   adaptive phenotypic response to environmental heterogeneity.
   Consequently, maternal experience is translated into variation in
   offspring fitness.}},
