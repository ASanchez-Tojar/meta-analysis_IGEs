
@ARTICLE{Desjonquères2019,
author={Desjonquères, C. and Speck, B. and Rodríguez, R.L.},
title={Signalling interactions during ontogeny are a cause of social plasticity in Enchenopa treehoppers (Hemiptera: Membracidae)},
